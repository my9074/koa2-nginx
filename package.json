{
  "name": "koa2-nginx",
  "version": "1.0.5",
  "description": "This is an http-proxy based egg proxy middleware that can be used after bodyparse",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint --fix && jest",
    "lint": "eslint --format node_modules/eslint-friendly-formatter .",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node verify_commit_msg.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/my9074/koa2-nginx.git"
  },
  "author": "my9074 <313231068@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/my9074/koa2-nginx/issues"
  },
  "homepage": "https://github.com/my9074/koa2-nginx#readme",
  "devDependencies": {
    "chalk": "^2.4.1",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-egg": "^7.3.1",
    "eslint-friendly-formatter": "^4.0.1",
    "jest": "^24.7.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "lint-staged": "^7.2.0",
    "nyc": "^11.0.2",
    "querystring": "^0.2.0",
    "supertest": "^3.1.0",
    "yorkie": "^1.0.3"
  },
  "dependencies": {
    "http-proxy": "^1.18.0",
    "moment": "^2.22.2",
    "winston": "^2.4.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
